<section class="content home">
  <div class="container-fluid">
    <div class="block-header">
      <h2>Daily Code Byte</h2>
      <!--<small class="text-muted">Try on this Daily Code Byte</small>-->
    </div>
    <div class="row clearfix">
      <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
        <div class="card">
          <div class="body">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs tab-nav-right" role="tablist">
              <li role="presentation" class="active"><a href="#tabProblem" data-toggle="tab" aria-expanded="false">Problem</a></li>
              <li role="presentation" class=""><a href="#tabSubmissions" data-toggle="tab" aria-expanded="true">Submissions</a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
              <div role="tabpanel" class="tab-pane fade in active" id="tabProblem">
                <div class="tabheader">
                  {{log problem}}
                  <h2>{{problem.name}}</h2>
                </div>
                <p>{{markdown-to-html problem.content.description}}</p>
                <hr />
                {{#if problem.content.input-format}}
                  <div id="inputFormat">
                    <span><b>Input Format:</b></span>
                    {{markdown-to-html problem.content.input-format}}
                  </div>
                {{/if}}
                {{#if problem.content.constraints}}
                  <div id="constraint">
                    <span><b>Constraints:</b></span>
                    <span>{{problem.content.constraints}}</span>
                  </div>
                {{/if}}
                {{#if problem.content.output-format}}
                  <div id="outputFormat">
                    <span><b>Output Format:</b></span>
                    {{markdown-to-html problem.content.output-format}}
                  </div>
                {{/if}}
                <hr />
                {{#if problem.content.sample-input}}
                  <div id="sampleInput">
                    <span><b>Sample Input:</b></span>
                    <pre>{{problem.content.sample-input}}</pre>
                  </div>
                {{/if}}
                {{#if problem.content.sample-output}}
                  <div id="sampleOutput">
                    <span><b>Sample Output:</b></span>
                    <pre>{{problem.content.sample-output}}</pre>
                  </div>
                {{/if}}
                {{#if problem.content.explanation}}
                  <div id="sampleOutput">
                    <span><b>Explanation:</b></span>
                    <span>{{problem.content.explanation}}</span>
                  </div>
                {{/if}}
                <hr />
                <div class="row clearfix">
                  <div class="col-md-12 pt-detail"><ul class="pt-category">
                    <li>
                      <a href="#">
                        <span>Points</span>
                        <i class="material-icons">star</i>
                        <span>100</span>
                      </a>
                    </li>
                    <li>
                      <a href="#">
                        <span>Time</span>
                        <i class="material-icons">access_time</i>
                        <span>10 Min</span>
                      </a>
                    </li>
                    <li>
                      <a href="#">
                        <span>Max</span>
                        <i class="material-icons">trending_up</i>
                        <span>100/100</span>
                      </a>
                    </li>
                    <li>
                      <a href="#">
                        <span>Total</span>
                        <i class="material-icons">group</i>
                        <span>360</span>
                      </a>
                    </li>
                  </ul></div>
                </div>
              </div>


              <div role="tabpanel" class="tab-pane fade" id="tabSubmissions">
                {{#problem-my-submissions submissions=submissions}}{{/problem-my-submissions}}
              </div>
            </div>
          </div>
        </div>

        <div class="row clearfix">

          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="card">
              <div class="header">
                <h2>Code Now</h2>
                <ul class="header-dropdown m-r--5">
                  <li class="dropdown"> <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false"><i class="zmdi zmdi-more-vert"></i></a>
                    <ul class="dropdown-menu pull-right">
                      <li><a href="javascript:void(0);">Full screen</a></li>

                    </ul>
                  </li>
                </ul>
              </div>
              <div class="body">
                       <textarea id="ckeditor">
                                <h2>CB Code Editor</h2>

                            </textarea>

                <hr />

                <a href="javascript:void(0);" class="btn btn-raised btn-sm" role="button">Reset</a> <a href="javascript:void(0);" class="btn btn-raised g-bg-blush2 btn-sm" role="button">Run</a>
                <a href="javascript:void(0);" class="btn btn-raised g-bg-blush2 btn-sm" role="button">Submit</a>


              </div>
            </div>
          </div>
        </div>

      </div>
      <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
        <div class="card">
          <div class="header">
            <h2> Best Submission </h2>
            <ul class="header-dropdown m-r--5">
