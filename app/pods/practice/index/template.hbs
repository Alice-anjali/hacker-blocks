<section class="content">
  <div class="container-fluid">
      <div class="block-header">
        <h2>This Week's Daily Dose</h2>
        <small class="text-muted">Take these challenges and prove your mettle </small>
      </div>  
    <div class="row clearfix">
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card">
          <div class="body">
            <div class="row clearfix">
               {{log model}}
                {{#each model as |problem|}}
                  {{#problem-view isWeekly=true link='practice.weekly-problem.index' problem=problem}}{{/problem-view}}
                {{/each}}
            </div>
          </div>
        </div>
    </div>
  </div>
</div>
</section>
